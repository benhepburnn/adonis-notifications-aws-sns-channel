{
  "name": "@benhepburn/adonis-notifications-aws-sns-channel",
  "description": "An AWS SNS notification channel for @benhepburn/adonis-notifications",
  "version": "1.2.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy:templates",
    "release": "np --no-cleanup",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "notifications",
    "sms",
    "email",
    "adonis js",
    "provider",
    "service",
    "sns"
  ],
  "author": "benhepburn",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@aws-sdk/client-sns": "^3.687.0",
    "@aws-sdk/credential-providers": "^3.687.0",
    "@benhepburn/adonis-notifications": "^0.5.1",
    "@japa/assert": "^2.1.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.9.2",
    "@types/node": "^20.17.6",
    "c8": "^9.1.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.1",
    "np": "^9.2.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0",
    "@aws-sdk/client-sns": "^3.687.0",
    "@aws-sdk/credential-providers": "^3.687.0",
    "@benhepburn/adonis-notifications": "^0.5.1"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config"
}
